{
  "name": "lambda-assistant",
  "displayName": "lambda assistant",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lambda-assistant.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "lambdasView.refresh",
        "title": "Refresh",
        "icon": {
          "dark": "resources/white-refresh.svg",
          "light": "resources/refresh.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lambda-assistant-acitivity-bar",
          "title": "Lambda Assistant",
          "icon": "resources/main-icon.svg"
        }
      ]
    },
    "views": {
      "lambda-assistant-acitivity-bar": [
        {
          "id": "lambdasView",
          "name": "Lambdas",
          "contextualTitle": "Package xxx"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "lambdasView.refresh",
          "when": "view == lambdasView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.379.1"
  }
}
